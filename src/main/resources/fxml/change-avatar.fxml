<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="300.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChangeAvatarController">
      <ImageView fx:id="avatar" fitHeight="180.0" fitWidth="180.0" layoutX="100.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="previous" layoutX="18.0" layoutY="114.0" mnemonicParsing="false" style="-fx-background-color: #f39c63;" text="Пред.">
         <font>
            <Font name="Georgia" size="16.0" />
         </font>
      </Button>
      <Button fx:id="next" layoutX="295.0" layoutY="115.0" mnemonicParsing="false" style="-fx-background-color: #f39c63;" text="След.">
         <font>
            <Font name="Georgia" size="16.0" />
         </font>
      </Button>
      <Button fx:id="save" layoutX="49.0" layoutY="244.0" mnemonicParsing="false" style="-fx-background-color: #f39c63;" text="Сохранить">
         <font>
            <Font name="Georgia" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="248.0" layoutY="244.0" mnemonicParsing="false" style="-fx-background-color: #f39c63;" text="Отмена">
         <font>
            <Font name="Georgia" size="16.0" />
         </font>
      </Button>
</AnchorPane>
